      "object-curly-spacing": [
        "warn",
        "never"
      ],
      "quotes": [
        "warn",
        "single"
      ]
    }
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie < 11",
    "not op_mini all"
  ]
